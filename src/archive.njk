---
layout: default.html
permalink: /archive/
---

<h1>Archive</h1>

<h2>Posts</h2>
<ul>
{% for post in collections.posts | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a> - {{ post.date | readableDate }}</li>
{% endfor %}
</ul>

<h2>Links</h2>
<ul>
{% for link in collections.links | reverse %}
  <li><a href="{{ link.data.url }}">{{ link.data.title }}</a> - {{ link.date | readableDate }}</li>
{% endfor %}
</ul>